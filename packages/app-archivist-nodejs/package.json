{
  "name": "@xyo-network/app-archivist-nodejs",
  "version": "0.1.109",
  "description": "The CLI interface to spin up an archivist on the XYO network",
  "scripts": {
    "test": "export NODE_ENV=test && jest --no-cache --forceExit --coverage --verbose --detectOpenHandles",
    "build": "tsc",
    "prepublishOnly": "tsc -b .",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "bin": {
    "xyo-archivist": "bin/xyo-archivist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"Build must pass before committing\" && yarn build && yarn tslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/app-archivist-nodejs.git"
  },
  "keywords": [
    "xyo",
    "xyo-core",
    "xyo-nodejs"
  ],
  "author": "Ryan Pillsbury",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/app-archivist-nodejs/issues"
  },
  "homepage": "https://github.com/XYOracleNetwork/app-archivist-nodejs#readme",
  "dependencies": {
    "@xyo-network/api-archivist-graphql": "^0.1.109",
    "@xyo-network/db-archivist-sql": "^0.1.108",
    "@xyo-network/sdk-archivist-nodejs": "^0.1.108",
    "@xyo-network/sdk-core-nodejs": "^0.1.106",
    "config": "^2.0.1",
    "js-yaml": "^3.12.0",
    "leveldown": "^4.0.1",
    "levelup": "^3.1.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/leveldown": "^4.0.0",
    "@types/levelup": "^3.1.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "husky": "^1.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^3.1.6"
  },
  "gitHead": "63b8f92a41e7ff224fc21b52a7d996128cccd498"
}
