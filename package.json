{
  "name": "@xyo-network/sdk-archivist-nodejs",
  "version": "0.1.5",
  "description": "An archivist implementation on the xyo network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "export NODE_ENV=test && jest --no-cache --forceExit --coverage --verbose --detectOpenHandles",
    "build": "tsc -p tsconfig.build.json",
    "build-ci": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn tslint",
    "preversion": "yarn tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"Build must pass before committing\" && yarn build-ci && yarn tslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs.git"
  },
  "keywords": [
    "xyo",
    "xyo-core",
    "xyo-nodejs"
  ],
  "author": "Ryan Pillsbury",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs/issues"
  },
  "homepage": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs#readme",
  "dependencies": {
    "@xyo-network/sdk-core-nodejs": "0.1.94",
    "lodash": "^4.17.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "@types/uuid": "^3.4.4",
    "husky": "^1.1.1",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^3.1.2"
  }
}
