{
  "name": "@xyo-network/sdk-archivist-nodejs",
  "version": "0.1.4",
  "description": "An archivist implementation on the xyo network",
  "scripts": {
    "test": "export NODE_ENV=test && jest --no-cache --forceExit --coverage --verbose --detectOpenHandles",
    "build": "tsc -p tsconfig.build.json",
    "build-ci": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "precommit": "echo \"Build must pass before committing\" && yarn build-ci && yarn tslint",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn tslint",
    "preversion": "yarn tslint",
    "start": "bin/xyo-archivist -p 11000 -g 11001 -d ./archivistdata"
  },
  "bin": {
    "xyo-archivist": "bin/xyo-archivist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs.git"
  },
  "keywords": [
    "xyo",
    "xyo-core",
    "xyo-nodejs"
  ],
  "author": "Ryan Pillsbury",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs/issues"
  },
  "homepage": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs#readme",
  "dependencies": {
    "apollo-server": "^2.1.0",
    "commander": "^2.18.0",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.0",
    "leveldown": "^4.0.1",
    "levelup": "^3.1.1",
    "lodash": "^4.17.11",
    "node-rsa": "^1.0.1",
    "@xyo-network/api-originchain-graphql": "0.1.3",
    "@xyo-network/sdk-core-nodejs": "0.1.6"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/graphql": "^14.0.0",
    "@types/leveldown": "^4.0.0",
    "@types/levelup": "^3.1.0",
    "@types/node-rsa": "^0.4.3",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "husky": "^1.1.1",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^3.1.2"
  }
}
