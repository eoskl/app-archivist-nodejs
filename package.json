{
  "name": "sdk-archivist-nodejs",
  "version": "0.1.0",
  "description": "An archivist implementation on the xyo network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "export NODE_ENV=test && jest --no-cache --forceExit --coverage --verbose --detectOpenHandles",
    "build": "yarn build-ts && yarn tslint",
    "build-ts": "tsc",
    "dist": "yarn install && tsc -p tsconfig.build.json && yarn tslint",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "precommit": "echo \"Build must pass before committing\" && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs.git"
  },
  "keywords": [
    "xyo",
    "xyo-core",
    "xyo-nodejs"
  ],
  "author": "Ryan Pillsbury",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs/issues"
  },
  "homepage": "https://github.com/XYOracleNetwork/sdk-archivist-nodejs#readme",
  "dependencies": {
    "@types/graphql": "^14.0.0",
    "apollo-server": "^2.1.0",
    "elliptic": "^6.4.1",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.0",
    "node-rsa": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2"
  }
}
